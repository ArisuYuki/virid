{
  "name": "@starry/vue",
  "version": "0.0.1",
  "description": "Vue adapter for Starry, projecting logic sovereignty to reactive UI",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@starry/core": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "inversify": "^7.11.0"
  },
  "peerDependenciesMeta": {
    "inversify": {
      "optional": true
    }
  },
  "scripts": {
    "build": "tsup index.ts --format esm,cjs --dts --clean --external vue --external @starry/core --external reflect-metadata --external inversify"
  }
}
